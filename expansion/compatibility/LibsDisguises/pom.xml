<project>
    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>

    <parent>
        <groupId>com.github.sirblobman.combatlogx.expansion.compatibility</groupId>
        <artifactId>compatibility-parent</artifactId>
        <version>11.0.0.0-SNAPSHOT</version>
    </parent>

    <artifactId>LibsDisguises</artifactId>
    <name>Compatibility Expansion: LibsDisguises</name>
    <description>An expansion for CombatLogX that adds compatibility for features in LibsDisguises.</description>

    <repositories>
        <!-- MD_5 Public Repository -->
        <repository>
            <id>md5-public</id>
            <url>https://repo.md-5.net/repository/public/</url>
        </repository>
    </repositories>

    <dependencies>
        <!-- LibsDisguises -->
        <dependency>
            <groupId>LibsDisguises</groupId>
            <artifactId>LibsDisguises</artifactId>
            <version>10.0.24</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>

    <build>
        <finalName>LibsDisguises Compatibility</finalName>
    </build>
</project>
