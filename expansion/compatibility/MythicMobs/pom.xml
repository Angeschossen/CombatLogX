<project>
    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>

    <parent>
        <groupId>com.github.sirblobman.combatlogx.expansion.compatibility</groupId>
        <artifactId>compatibility-parent</artifactId>
        <version>11.0.0.0-SNAPSHOT</version>
        <relativePath>../pom.xml</relativePath>
    </parent>

    <artifactId>MythicMobs</artifactId>
    <name>Compatibility Expansion: MythicMobs</name>
    <description>An expansion for CombatLogX that allows you to set which MythicMobs entities tag players.</description>

    <repositories>
        <!-- Lumine Releases -->
        <repository>
            <id>lumine</id>
            <url>https://mvn.lumine.io/repository/maven-public/</url>
        </repository>
    </repositories>

    <dependencies>
        <!-- MythicMobs -->
        <dependency>
            <groupId>io.lumine.xikage</groupId>
            <artifactId>MythicMobs</artifactId>
            <version>4.11.0</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>

    <build>
        <finalName>MythicMobs Compatibility</finalName>
    </build>
</project>